---
title: Register in to MessegusO
stylesheets:
  - ./form.css
  - ./link.css
  - ./main.css
  - ./button.css
---
{{!< base}}

<main>
    <form class="form__wrapper">
        <h4 class="form__title">{{title}}</h4>
        <input id="first_name" class="form__input" placeholder="first name"></input>
        <input id="second_name" class="form__input" placeholder="second name"></input>
        <input id="email" class="form__input" placeholder="email"></input>
        <input id="phone" class="form__input" placeholder="phone"></input>
        <input id="password" type="password" class="form__input" placeholder="password"></input>
        {{> link href="./about.hbs" linkText="about"}}
        {{> button title="Sign up"}}
    </form>
</main>