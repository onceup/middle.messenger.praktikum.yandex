---
title: Sign up
css:
  - form.css
  - button.css
  - link.css
  - main.css
---

{{!< main}}

<main>
  <form class="form">
    <h2 class="form__title">{{title}}</h2>
    <input id="email" class="form__input" placeholder="email" />
    <input id="login" class="form__input" placeholder="login" />
    <input id="first_name" class="form__input" placeholder="first_name" />
    <input id="second_name" class="form__input" placeholder="second_name" />
    <input id="phone" class="form__input" placeholder="phone" />
    <input id="password" class="form__input" placeholder="phone" />
    <div class="form__button">
      {{> button type="submit" title="Sign up" href="./main.hbs"}}
    </div>
    {{> link href="./signIn.hbs" linkText="Sign in"}}
  </form>
</main>
